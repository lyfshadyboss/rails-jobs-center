<div class="panel panel-info student-panel-info">
  <div class="panel-heading">编辑简历</div>

  <div class="panel-body"></div>

  <%= form_for([@student, @resume],
               url: student_resume_path(@resume),
               html: {class: 'form-horizontal', role: 'form'}) do |f| %>
      <%= render partial: "resumes/form", locals: {form: f} %>
  <% end %>

</div>

