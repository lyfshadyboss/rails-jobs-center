<div class="panel panel-info admin-panel-info">
  <div class="panel-heading">导入学生帐户</div>
  <div class="panel-body">用于将统计后的学生数据(excel表格)导入</div>

  <%= form_for(current_admin, url: '/admin/import_student_accounts', method: 'post',
               html: {class: 'form-horizontal', role: 'form'}) do |f| %>
      <div class="form-group">
        <div class="col-sm-offset-1 col-sm-2">
          <%= f.file_field :student_infos, class: "form-control" %>
        </div>
      </div>

      <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
          <button type="submit" class="btn btn-default">提交</button>
        </div>
      </div>
  <% end %>
</div>